<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>context_test</servlet-name>
    <servlet-class>newnew.study.Main</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>context_test</servlet-name>
