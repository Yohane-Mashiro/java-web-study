<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>context_test</servlet-name>
    <servlet-class>newnew.study.SendRedirect</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>context_test</servlet-name>
    <url-pattern>/test</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>newnew</servlet-name>
    <servlet-class>newnew.study.Servlet_context</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>newnew</servlet-name>
    <url-pattern>/newnew</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>db</servlet-name>
    <servlet-class>newnew.study.GetDatabase</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>db</servlet-name>
    <url-pattern>/db</url-pattern>
  </servlet-mapping>
  
  <context-param>
    <param-name>db</param-name>
    <param-value>jdbc:mysql//localhost:3306/newnew</param-value>
  </context-param>
</web-app>
